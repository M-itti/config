[alias]
	l = log
	s = status
	d = diff
	co = checkout
	b = branch
	c = checkout
	q = "!f() { \
	branch=$(git symbolic-ref --short HEAD); \
	if [ \"$branch\" = \"main\" ] || [ \"$branch\" = \"master\" ]; then \
		echo \"ðŸš« Aborting: You are on the $branch branch!\"; \
		exit 1; \
	fi; \
	git add . && \
	git commit -am \"Auto: $(date '+%Y-%m-%d %H:%M:%S')\" && \
	git push origin \"$branch\"; \
	}; f"
